{
  "name": "enzolarosa/translator",
  "description": "Make easy the translation.",
  "keywords": [
    "laravel",
    "nova",
    "translator"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/contracts": "^9.0|^10.0",
    "friendsofphp/php-cs-fixer": "^3.14.4",
    "ext-json": "*"
  },
  "require-dev": {
    "nunomaduro/collision": "^6.4",
    "nunomaduro/larastan": "^2.5.1",
    "orchestra/testbench": "^7.22.1",
    "pestphp/pest": "^1.22.5",
    "pestphp/pest-plugin-laravel": "^1.4",
    "phpstan/extension-installer": "^1.2",
    "phpstan/phpstan-deprecation-rules": "^1.1.2",
    "phpstan/phpstan-phpunit": "^1.3.10",
    "phpunit/phpunit": "^9.6.4"
  },
  "autoload": {
    "psr-4": {
      "enzolarosa\\Translator\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "enzolarosa\\MqttBroadcast\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "enzolarosa\\Translator\\TranslatorServiceProvider"
      ],
      "aliases": {
        "Translator": "enzolarosa\\Translator\\Facades\\Translator"
      }
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
