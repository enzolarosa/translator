{
  "name": "enzolarosa/translator",
  "description": "Make easy the translation.",
  "keywords": [
    "laravel",
    "nova",
    "translator"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0|^8.1"
  },
  "autoload": {
    "psr-4": {
      "enzolarosa\\Translator\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "enzolarosa\\Translator\\TranslatorServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
